<div class='row'>
  <div class='medium-12 columns company'>
    <div class='company-bgimage'>
      <% if @company.company_image_url %>
        <%= image_tag @company.company_image_url %>
      <% else %>
      <% end %>
    </div>
    <div class='medium-10 medium-offset-1 columns'><br>
      <h3 class='text-center'>Editing Your Interview Experienced with <%= @company.name %></h3>
      <%= render 'form' %>
      <%= link_to 'Back', company_path(@company), class: "secondary button" %>
    </div>
  </div> 
</div>